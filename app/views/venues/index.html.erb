<%= render "new_venues" %>

<div id="contents">
    
  <h1>All venues</h1>

  <table>
    <tr>
      <th>Name</th>
      <th>Address</th>
    </tr>

  <% @venues.each do |venue| %>
    <tr>
      <td><%= link_to venue.name, venue %></td>
      <td><%= venue.address %></td>
    </tr>
  <% end %>
  </table>

  <br />

  <%= link_to 'New Venue', new_venue_path %>

</div>