<%= render "created_events" %><div id="contents">  <h1>My Stuff</h1>  <i><%= t('views.profiles.no_name_set') if @profile.name.empty? %></i><%= @profile.name %> | <%= link_to t('views.profiles.change_my_profile'), edit_profile_path(@profile) %> <br \>  <%= @profile.user.email %> | <%= link_to t('views.profiles.change_my_email_pw'), edit_user_path(current_user) %> <br \>  <br \>  <%= t('views.profiles.calendar_msg') unless @profile.events.any? %>  <% if @profile.events.upcoming.any? %>    <h2> <%= t('views.profiles.my_upcoming_shows') %> </h2>    <%= render @profile.events.upcoming %>  <% end %></div>